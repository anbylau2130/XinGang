@model IEnumerable<Com.XinGang.Model.Model.Menu>

    <!-- 网页标题(菜单) -->
    <div id='pdv_16700' class='pdv_class' title='栏目导航' style='width: 200px; height: 446px; top: 15px; left: 15px; z-index: 4'>
        <div id='spdv_16700' class='pdv_content' style='overflow: hidden; width: 100%; height: 100%'>
            <div class="pdv_border" style="border:0px;height:100%;background:#ffffff url(@Url.Content("~/Resource/base/border/604/images/l.png")) repeat-y">
                <div style="height:100%;background:url(@Url.Content("~/Resource/base/border/604/images/r.png")) right repeat-y">
                    <div style="height:39px;background:url(@Url.Content("~/Resource/base/border/604/images/top.png")) 0px 0px no-repeat">
                        <div style="float: left; font: 14px/41px 'microsoft yahei','SimSun',Arial,Sans-Serif; padding-left: 32px; color: #fff;">
                            栏目导航
                        </div>
                        <div style="float:right;width:60px;height:39px;text-align:right;background:url(@Url.Content("~/Resource/base/border/604/images/top.png")) -840px 0px no-repeat">
                            <a href="-1" style="display: none">
                                <img src="@Url.Content("~/Resource/base/border/604/images/more.png")" style="border:0px;margin:18px 20px 0px 0px;" /></a>
                        </div>
                    </div>
                    <div style="margin: 0px 3px; padding: 0px">
                        
                        <link href="@Url.Content("~/Resource/page/templates/css/pagetitlemenu_dolphin.css")" rel="stylesheet" type="text/css" />
                        <div class="titlemenu_dolphin">
                            @foreach (var menu in Model)
                            {
                                <a href="javascript:changemenu(@menu.ID,'@menu.Text')"  target="_self" class="titlemenu_dolphin">@menu.Text</a>
                            }
                        </div>

                    </div>
                </div>
            </div>
            <div style="margin-top:-12px;height:12px;line-height:12px;background:url(@Url.Content("~/Resource/base/border/604/images/top.png")) 0px -218px no-repeat">&nbsp;</div>
            <div style="float:right;margin-top:-12px;width:12px;height:12px;line-height:12px;background:url(@Url.Content("~/Resource/base/border/604/images/top.png")) -888px -218px no-repeat">&nbsp;</div>
        </div>
    </div>

    <!-- 当前位置提示条 -->
    <div id='pdv_16702' class='pdv_class' title='' style='width: 750px; height: 39px; top: 18px; left: 230px; z-index: 5'>
        <div id='spdv_16702' class='pdv_content' style='overflow: hidden; width: 100%; height: 100%'>
            <div class="pdv_border" style="border:0px;background:url(@Url.Content("~/Resource/base/border/788/images/title.png")) 0 0 no-repeat;padding:0px;margin:0px;height:100%;">
                <link href="@Url.Content("~/Resource/page/templates/css/nav.css")" rel="stylesheet" type="text/css" />
                <div id="nav">
                    您现在的位置：<a href="@Url.Content("~/Home/Index")">企业形象成品网站</a> &gt; <span><a href="@Url.Content("~/Home/" + ViewData["PageName"])">@ViewData["PageTitle"]</a></span>&gt; <span id="currentpath">@ViewData["menuname"] </span>
                </div>
            </div>
        </div>
    </div>

