
@model IEnumerable<Com.XinGang.Model.Model.ProductType>

<script type="text/javascript">

    $(function () {
        var id = '@ViewData["id"]';

        change(id);
    });

    function change(id) {
        var url1 = '@Url.Content("~/Home/GetProduct")' + '?id=' + id;
        $.ajax({
            url: url1,
            type: "POST",
            dataType: "json",
            success: function (data) {
                console.log(data);
                var htmlproductlist='';
                for(var i =0;i<data.length;i++)
                {
                    htmlproductlist += '<div class="productquery_dolphin">' +
                                         '<div class="productquery_dolphin_top">' +
                                          '<div class="fang" style="width:160px;height:120px">' +
                                             '<div class="picFit" style="width:160px;height:120px">' +
                                                '<a href="../../product/html/?82.html" target="_self" >' +
                                                 '<img src="@Url.Content("~/Resource/product/pics/")' + data[i].PicURL + '" style="width:160px;height:120px" border="0" />' +
                                                '</a>' +
                                             '</div>' +
                                           '</div>' +
                                         '</div>' +
                                       '<div class="productquery_dolphin_bottom">' +
                                      '<a href="javascript:openproduct('+data[i].ID+')" target="_self" class="prodtitle"   >' + data[i].Name + '</a>' +
                                    '</div></div>';
                }
                $("#productList").html(htmlproductlist);
            }
        });
    }
  
</script>
<div id='content' style='width:1002px;min-height:517px;background:url(@Url.Content("~/Resource/effect/source/bg/cbg.png")) repeat-y;margin:0px auto'>


